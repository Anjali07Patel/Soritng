#include<bits/stdc++.h>
using namespace std;
void swap(int arr[] , int pos1, int pos2){
	int temp;
	temp = arr[pos1];
	arr[pos1] = arr[pos2];
	arr[pos2] = temp;
}
int partition(int a[], int l, int h)
{
    int pivot_item = a[l];
    int temp;
    int i = l;
    int j = h;

    while (i < j)
    {
        
        while(a[i] <= pivot_item) i++;
        
        while(a[j] > pivot_item) j--;

        if(i < j){
            swap(a,i,j);
        }
    }
    
    a[l] = a[j];
    a[j] = pivot_item;
    return j;
}

void QuickSort(int a[], int l, int h)
{
    int n = h-l;
    if(h > l){
        int pivot = partition(a, l, h);
        QuickSort(a, l, pivot-1);
        QuickSort(a, pivot+1, h);
    }

}

int main()
{
    int arr[] = {4,6,2,5,7,9,1,3};
    int n = sizeof(arr)/sizeof(arr[0]);

    QuickSort(arr, 0, n-1);

    for(int i=0; i<n; i++)
    {
        cout<<arr[i]<<" ";
    }
    cout<<endl;
}
